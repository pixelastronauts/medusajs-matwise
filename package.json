{
	"name": "medusa-2.0-boilerplate-backend",
	"version": "0.0.2",
	"description": "A Prebaked Medusa 2.0 backend",
	"author": "Rasmus Puls",
	"license": "MIT",
	"keywords": [
		"sqlite",
		"postgres",
		"typescript",
		"ecommerce",
		"headless",
		"medusa"
	],
	"scripts": {
		"build": "medusa build && node src/scripts/postBuild.js",
		"seed": "medusa exec ./src/scripts/seed.ts",
		"seed:tax": "medusa exec ./src/scripts/seed-tax-setup.ts",
		"verify:tax": "medusa exec ./src/scripts/verify-tax-setup.ts",
		"cleanup:tax": "medusa exec ./src/scripts/cleanup-tax-providers.ts",
		"fix:nl": "medusa exec ./src/scripts/fix-nl-tax-region.ts",
		"reset:tax": "medusa exec ./src/scripts/reset-tax-regions.ts",
		"ib": "init-backend",
		"start": "init-backend && cd .medusa/server && medusa start --verbose",
		"dev": "medusa develop",
		"email:dev": "email dev --dir=./src/modules/email-notifications/templates --port=3002",
		"test:integration:http": "TEST_TYPE=integration:http NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
		"test:integration:modules": "TEST_TYPE=integration:modules NODE_OPTIONS=--experimental-vm-modules jest --silent=false --runInBand --forceExit",
		"test:unit": "TEST_TYPE=unit NODE_OPTIONS=--experimental-vm-modules jest --silent --runInBand --forceExit"
	},
	"dependencies": {
		"@medusajs/admin-sdk": "2.10.2",
		"@medusajs/cli": "2.10.2",
		"@medusajs/dashboard": "2.10.2",
		"@medusajs/draft-order": "^2.10.2",
		"@medusajs/framework": "2.10.2",
		"@medusajs/medusa": "2.10.2",
		"@medusajs/notification-sendgrid": "2.10.2",
		"@medusajs/workflow-engine-redis": "2.10.2",
		"@mikro-orm/core": "6.4.3",
		"@mikro-orm/knex": "6.4.3",
		"@mikro-orm/migrations": "6.4.3",
		"@mikro-orm/postgresql": "6.4.3",
		"@react-email/components": "^0.0.26",
		"@rokmohar/medusa-plugin-meilisearch": "1.3.5",
		"@variablevic/mollie-payments-medusa": "^0.0.13",
		"awilix": "^8.0.1",
		"medusajs-launch-utils": "0.0.18",
		"minio": "^8.0.3",
		"pdfmake": "^0.2.20",
		"pg": "^8.13.1",
		"resend": "4.0.1",
		"ulid": "^2.3.0"
	},
	"devDependencies": {
		"@medusajs/test-utils": "2.10.2",
		"@medusajs/ui": "^4.0.2",
		"@mikro-orm/cli": "6.4.3",
		"@swc/core": "1.5.7",
		"@swc/jest": "^0.2.37",
		"@types/jest": "^29.5.14",
		"@types/node": "^20.17.10",
		"@types/pdfmake": "^0.2.12",
		"@types/react": "^18.3.18",
		"@types/react-dom": "^18.3.5",
		"jest": "^29.7.0",
		"prop-types": "^15.8.1",
		"react": "^18.3.1",
		"react-dom": "^18.3.1",
		"react-email": "^3.0.4",
		"ts-node": "^10.9.2",
		"typescript": "^5.7.2",
		"vite": "^5.4.11"
	},
	"packageManager": "bun@1.3.1",
	"engines": {
		"node": "22.x",
		"bun": "1.x"
	}
}
